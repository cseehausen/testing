{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "path": "./1.csv",
          "file_format": "csv",
          "tag": "1",
          "columnDescription": {
            "KEY": 0,
            "en": 1
          },
          "language": "en"
        },
        {
          "path": "./2.csv",
          "file_format": "csv",
          "tag": "2",
          "columnDescription": {
            "KEY": 0,
            "en": 1
          },
          "language": "en"
        },
        {
          "path": "./3.json",
          "file_format": "i18next_json",
          "tag": "3",
          "language": "en"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "path": "./<language>.csv",
          "file_format": "csv",
          "tag": "1",
          "excludeHeaderRow": true,
          "excludeBaseLang": true,
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./<language>.csv",
          "file_format": "csv",
          "tag": "2",
          "excludeHeaderRow": true,
          "excludeBaseLang": true,
          "exclude_languages": [
            "en"
          ]
        },
        {
          "path": "./<language>.json",
          "file_format": "i18next_json",
          "tag": "3",
          "exclude_languages": [
            "en"
          ]
        }
      ]
    }
  }
}